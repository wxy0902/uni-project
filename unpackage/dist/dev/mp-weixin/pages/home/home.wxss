
.search-wrap{
	padding: 20rpx;
	background-color: #FFFFFF;
}
/* 滑动scroll导航条 */
.scroll-bar-find {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  white-space: nowrap;
  -webkit-flex-wrap: nowrap;
          flex-wrap: nowrap;
  overflow: hidden;
  /* background: #FF6699; */
}
/* 滑动scroll导航条项*/
.scroll-bar-finditem {
  display: inline-block;
  width: 20%;
  height: 86rpx;
  line-height: 86rpx;
  text-align: center;
  font-size: 32rpx;
  /* color: rgba(255, 255, 255, 0.6); */
  border-bottom: 4rpx transparent solid;
}
.scroll-bar-finditemsh {
  color: #FFFFFF;
  border-bottom: 4rpx #FFFFFF solid;
  font-size: 36rpx;
  font-weight: bold;
}

